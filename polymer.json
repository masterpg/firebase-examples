{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/auth-view.html",
    "src/file-upload-view.html",
    "src/my-view1.html",
    "src/my-view2.html",
    "src/my-view3.html",
    "src/my-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "firebase-config.js"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es6-bundled",
      "addServiceWorker": false
    }
  ]
}
